{
  "manifest_version": 3,
  "name": "Revife",
  "version": "1.0",
  "description": "",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./Images/icon-16.png",
      "32": "./Images/icon-32.png",
      "48": "./Images/icon-48.png",
      "128": "./Images/icon-128.png"
    }
  },
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.sellpy.com/search*"],
      "js": ["content.js"]
    }
  ]
}
